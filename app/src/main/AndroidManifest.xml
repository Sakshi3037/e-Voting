<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.evoting" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Navigation"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.myTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".User_info"
            android:label="@string/title_activity_user_info" >
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".Info"
            android:label="@string/title_activity_info" >
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".Authentication"
            android:label="@string/title_activity_authentication" >
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".SessionExpire"
            android:label="@string/title_activity_session_expire" >
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".Twitter"
            android:label="@string/title_activity_twitter" >
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".MainActivity"
            android:label="eVoting" >
        </activity>
        <activity
            android:name=".Analysis"
            android:label="@string/title_activity_analysis" >
        </activity>
    </application>

</manifest>
